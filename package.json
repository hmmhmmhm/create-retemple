{
  "name": "create-retemple",
  "version": "0.0.4",
  "description": "Create an on-the-fly template that makes it easy to create a React Library.",
  "type": "module",
  "main": "./bin/index.js",
  "scripts": {
    "dev": "rm -rf ./e2e && mkdir -p e2e && npm run build && cd ./e2e && node ../bin/index.js",
    "build": "swc ./src -d ./dist",
    "release": "release-it"
  },
  "bin": {
    "create-retemple": "./bin/index.js"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "files": [
    "bin/*",
    "dist/*",
    "template/*"
  ],
  "author": "hmmhmmhm",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "release-it": "^17.1.1"
  },
  "release-it": {
    "hooks": {
      "before:init": "npm run build"
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.2",
    "chalk": "^5.3.0",
    "tasuku": "^2.0.1"
  }
}
